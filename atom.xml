---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Christian Kirkham</title>
 <link href="http://www.ckirkham.com/atom.xml" rel="self"/>
 <link href="http://www.ckirkham.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://www.ckirkham.com/</id>
 <author>
   <name>Christian Kirkham</name>
   <email>im@ckirkham.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://www.ckirkham.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://www.ckirkham.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>